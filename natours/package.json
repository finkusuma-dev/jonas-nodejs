{
  "name": "natours",
  "version": "1.0.0",
  "description": "learning node, express, and mongoDb",
  "main": "built/server.js",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "test": "jest --verbose",
    "compile": "tsc",
    "compile-watch": "tsc --watch",
    "server:dev": "nodemon ./built/server.js",
    "server:prod": "NODE_ENV=production nodemon ./built/server.js",
    "start:dev": "npm-run-all --parallel compile-watch server:dev",
    "start:prod": "npm-run-all --parallel compile-watch server:prod"
  },
  "keywords": [
    "learn_node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
